/*
12번째 줄에 (n-j) 이 부분을 1,2,3,4,~.n 의 값으로 출력하고 싶은데 방법이 없을까요
*/
import java.util.Scanner;
public class Main {
    public static void main(String[] args) {
        Scanner sc =new Scanner(System.in);
        int n = sc.nextInt();
        for(int i=1; i<=n; i++){
            for(int j=n-i; j>=0; j--){
               System.out.print(i+ " * " + (n-j) + " = " + i*(n-j));
                 System.out.print(" / ");
            }
            System.out.println("");
         }
    }
}
